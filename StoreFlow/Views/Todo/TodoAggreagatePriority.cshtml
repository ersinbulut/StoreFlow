@{
    ViewData["Title"] = "TodoAggreagatePriority";
    Layout = "~/Views/Layout/Index.cshtml";
}

<style>
        .solid-bullet-list li:nth-child(5n+1):before {
      background: #04B76B;
    }
    .solid-bullet-list li:nth-child(5n+2):before {
      background: #FF5E6D;
    }
    .solid-bullet-list li:nth-child(5n+3):before {
      background: #0B94F7;
    }
    .solid-bullet-list li:nth-child(5n+4):before {
      background: #F5A623;
    }
    .solid-bullet-list li:nth-child(5n+5):before {
      background: #392C70;
    }
</style>
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <ul class="solid-bullet-list">
                    @Html.Raw(ViewBag.result)
                </ul>
            </div>
        </div>
    </div>
</div>